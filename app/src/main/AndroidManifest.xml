<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.destinyminiproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ThemeLight">
        <activity android:name=".StoryThreeListenActivity"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".StoryThreeReadActivity" />
        <activity android:name=".StoryTwoListenActivity" />
        <activity android:name=".MainActivity3" />
        <activity android:name=".StoryOneListenActivity" />
        <activity android:name=".StoryTwoReadActivity" />
        <activity android:name=".StoryOneReadActivity" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".MainActivity1" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmReceiver">
            <intent-filter>
                <action android:name="bhavi.action.DISPLAY_NOTIFICATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>